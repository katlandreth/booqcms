<%= form_for(@upload, url: {controller: "media", action: "create", :format => :js, entry_id: @entry.id}, remote: true, authenticity_token: true) do |f| %>
  <div>
  </br>
    <%= f.file_field :file, class: "file" %>
  </div>
  <div>
    <%= f.submit class: "upload-submit" %> <%= button_tag 'Reset', type: 'reset', id: 'cancel-upload' %>
  </div>
<% end %>
